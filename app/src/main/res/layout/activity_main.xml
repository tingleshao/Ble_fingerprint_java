<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.chongshao_mikasa.ble_fingerprint_java.MainActivity">

    <TextView
        android:text="TextView"
        android:id="@+id/beacon0"
        android:layout_width="150dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="450dp"
        android:layout_height="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_below="@+id/beacon0"
        android:layout_alignEnd="@+id/beacon0"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon1"
        android:layout_height="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_below="@+id/beacon1"
        android:layout_alignStart="@+id/beacon1"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon2"
        android:layout_height="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_below="@+id/beacon2"
        android:layout_alignStart="@+id/beacon2"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon3"
        android:layout_height="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/beacon3"
        android:layout_alignStart="@+id/beacon3"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon4" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/beacon4"
        android:layout_alignEnd="@+id/beacon4"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon5" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/beacon5"
        android:layout_alignStart="@+id/beacon5"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon6" />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/beacon6"
        android:layout_alignStart="@+id/beacon6"
        android:layout_marginTop="1dp"
        android:id="@+id/beacon7" />

    <com.example.chongshao_mikasa.ble_fingerprint_java.MySurfaceView
        android:id="@+id/gl_layout"
        android:orientation="horizontal"
        android:layout_height="175dp"
        android:layout_width="175dp"
        android:layout_marginLeft="250dp"
        />

    <FrameLayout
        android:id="@+id/mainLayout"
        android:orientation="vertical"
        android:layout_width="250dp"
        android:layout_height="250dp"
        />

    <TextView
        android:text="TextView"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/gl_layout"
        android:layout_alignStart="@+id/beacon7"
        android:layout_marginTop="13dp"
        android:id="@+id/rotationMsg" />

</RelativeLayout>
